// wnApp default configuration.
{

	// Application Name
	"title": "Default WNS WebApp",

	// Auto import
	"import": [],

	// Components
	"components": {},

	// Events
	"events": {

		"log": {
			"zone": {
				"*": true,
				"access": true,
				"mysql": true,
				"notfound": true,
				"trace": true,
				"exception": true,
				"stack": true
			},
			"eventName": "wnLog",
			"handler": "logHandler",
			"filters": ["filterLog"],
			"bubble": true
		},

		"exception": {
			"handler": "exceptionHandler",
			"eventName": "wnException"
		}

	}

}