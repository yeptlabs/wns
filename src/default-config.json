// Server configuration
{

	// Where the applications will be stored.
	"appDirectory": "app/",

	// Application load system configuration
	"app": {
		// Example of app load configuration (uncomment to create the default app)
		// "default": {
		// 	"appPath": "default/",
		// 	"domain": "*"
		// }
	},

	// Components
	"components": {
		// HTTP server configuration (remove/comment to disable)
		"http": {
			// Component class.
			"class": "wnHttp",
			// Listen on port and IP...
			"listen":[8000]
		}
	}

}